{
  "name": "@xjog/packages",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "npm": ">=8.19.4",
    "yarn": "^1.22.8",
    "node": ">=16.x"
  },
  "scripts": {
    "watch-all": "lerna run build ; lerna watch -- lerna run build --since",
    "link-all": "lerna exec --parallel yarn link",
    "unlink-all": "lerna exec --parallel --bail=false yarn unlink"
  },
  "devDependencies": {
    "lerna": "7.4.2"
  }
}